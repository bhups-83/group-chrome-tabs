{
  "manifest_version": 3,
  "name": "Tabstract",
  "version": "1.0.0",
  "description": "Read URLs of open tabs in the current window and group them by domain.",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "permissions": ["tabs", "tabGroups", "windows", "storage"],
  "action": {
    "default_title": "Tabstract",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png"
    }
  }
}

